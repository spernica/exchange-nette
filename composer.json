{
	"name": "h4kuna/exchange-nette",
	"type": "library",
	"description": "Exchange between currencies.",
	"homepage": "https://github.com/h4kuna/exchange-nette",
	"license": "MIT",
	"authors": [
		{
			"name": "Milan Matějček",
			"email": "milan.matejcek@gmail.com",
			"role": "Developer"
		}
